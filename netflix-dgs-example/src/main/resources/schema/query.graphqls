# All Available Queries
type Query {
    # Get All Users
    users: [User],
    # Get User by userId
    userById(id:ID): User,
    # Get All Posts
    posts: [Post],
    # Get Post by postId
    postById(id:ID): Post,
    # Get All Posts by userId
    postsByUserId(userId:ID): [Post]
    # Get All Comments
    comments: [Comment]
    # Get Comment by commentId
    commentById(id:ID): Comment
    # Get All Comments by postId
    commentsByPostId(postId:ID): [Comment]
}